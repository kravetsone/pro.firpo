{
    "$schema": "https://biomejs.dev/schemas/1.5.2/schema.json",
    "organizeImports": {
        "enabled": true
    },
    "linter": {
        "enabled": true,
        "rules": {
            "complexity": {
                "noStaticOnlyClass": "off"
            },
            "suspicious": {
                "useValidTypeof": "off",
                "noExplicitAny": "off"
            },
            "recommended": true
        }
    }
}
